@model IEnumerable<UserTest>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.TestLocation)</th>
            <th>@Html.DisplayNameFor(model => model.TestPrice)</th>
            <th>@Html.DisplayNameFor(model => model.TestDate)</th>
            <th>@Html.DisplayNameFor(model => model.ResultDate)</th>
            <th>@Html.DisplayNameFor(model => model.HomeAddress)</th>
            <th>@Html.DisplayNameFor(model => model.User)</th>
            <th>@Html.DisplayNameFor(model => model.Lab)</th>
            <th>@Html.DisplayNameFor(model => model.Branch)</th>
            <th>@Html.DisplayNameFor(model => model.Test)</th>
            <th>@Html.DisplayNameFor(model => model.City)</th>
            <th>@Html.DisplayNameFor(model => model.Area)</th>
            <th>@Html.DisplayNameFor(model => model.TestStatus)</th>
            <th>@Html.DisplayNameFor(model => model.IsActive)</th>
            <th>@Html.DisplayNameFor(model => model.CreatorId)</th>
            <th>@Html.DisplayNameFor(model => model.CreationDate)</th>
            <th>@Html.DisplayNameFor(model => model.ModifierId)</th>
            <th>@Html.DisplayNameFor(model => model.LastModifiedDate)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.TestLocation)</td>
                <td>@Html.DisplayFor(modelItem => item.TestPrice)</td>
                <td>@Html.DisplayFor(modelItem => item.TestDate)</td>
                <td>@Html.DisplayFor(modelItem => item.ResultDate)</td>
                <td>@Html.DisplayFor(modelItem => item.HomeAddress)</td>
                <td>@Html.DisplayFor(modelItem => item.User.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.Lab.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.Branch.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.Test.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.City.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.Area.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.TestStatus.Id)</td>
                <td>@Html.DisplayFor(modelItem => item.IsActive)</td>
                <td>@Html.DisplayFor(modelItem => item.CreatorId)</td>
                <td>@Html.DisplayFor(modelItem => item.CreationDate)</td>
                <td>@Html.DisplayFor(modelItem => item.ModifierId)</td>
                <td>@Html.DisplayFor(modelItem => item.LastModifiedDate)</td>
                <partial name="_Actions" model="@item" />
            </tr>
        }
    </tbody>
</table>
